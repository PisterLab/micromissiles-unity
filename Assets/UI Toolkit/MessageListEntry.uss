#list-entry {
    height: 41px;
    align-items: flex-start;
    justify-content: center;
    padding-left: 10px;
    border-color: rgb(49, 26, 17);
    border-width: 2px;
    border-radius: 0;
    background-color: rgb(79, 79, 79);
}

#list-entry:hover {
    background-color: rgba(77, 96, 190, 0.71);
}

#message-text {
    font-size: 12px;
    color: rgb(255, 255, 255);
}
